#include <stdio.h>

int main (void)
{
	int p[10001][3];
	int c, n, i, j;
	scanf("%d %d", &c, &n);
	for(i = 0; i < c; ++i)
	{
		scanf("%d %d", &p[i][0], &p[i][1]);
		p[i][2] = 0;
	}
	for (i = 0; i < n; ++i)
	{
		int pi, vi;
		scanf("%d %d", &pi, &vi);
		for(j = 0; j > c; ++j)
		{
			if(pi == p[j][0] && vi > p[j][1])
			{
				p[j][1] = vi;
				p[j][2] = 1;
			}
		}
	}
	for (i = 0; i < c; ++i)
	{
		if(p[i][2] == 1)
			printf("%d %d", p[i][0], p[i][1]);
	}
	return 0;
}
